<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite SYSTEM "UrlRewrite.dtd" >
<urlrewrite default-match-type="regex" use-query-string="true">
	<rule match-type="regex">
	    <from>^/object/?$</from>
	    <to>/metacat/object/</to>
	</rule>
	<rule match-type="regex">
	    <from>^/object/([^/\?]+)$</from>
	    <to>/metacat/object/$1</to>
	</rule>
	<rule match-type="regex">
	    <from>^/meta/([^/\?]+)/?$</from>
	    <to>/metacat/object/$1/meta/</to>
	</rule>
	<rule match-type="regex">
	    <from>^/resolve/([^/\?]+)/?$</from>
	    <to>/metacat/object/$1/resolve/</to>
	</rule>
	<rule match-type="regex">
	    <from>^/object/(\?qt=path.*)$</from>
	    <to>/metacat/object/$1</to>
	</rule>
	<rule match-type="regex">
	    <from>^/object(\?qt=path.*)$</from>
	    <to>/metacat/object/$1</to>
	</rule>
	<rule match-type="regex">
	    <from>^/object/(\?qt=solr.*)$</from>
	    <to>/mercury/object/$1</to>
	</rule>
	<rule match-type="regex">
	    <from>^/object(\?qt=solr.*)$</from>
	    <to>/mercury/object/$1</to>
	</rule>
	<rule match-type="regex">
	    <from>^/node/?$</from>
	    <to>/nodeList.xml</to>
	</rule>
	<outbound-rule>
	    <from>^/metacat/object/$</from>
	    <to>/object/</to>
	</outbound-rule>
	<outbound-rule>
	    <from>^/metacat/object/(.+)$</from>
	    <to>/object/$1</to>
	</outbound-rule>
	<outbound-rule>
	    <from>^/metacat/object/([^/\?]+)/meta$</from>
	    <to>/meta/$1</to>
	</outbound-rule>
	<outbound-rule>
	    <from>^/metacat/object/([^/\?]+)/resolve$</from>
	    <to>/resolve/$1</to>
	</outbound-rule>
	<outbound-rule>
	    <from>^/mercury/object/(.+)$</from>
	    <to>/object/$1</to>
	</outbound-rule>
	<outbound-rule>
	    <from>^/nodeList\.xml$</from>
	    <to>/node/</to>
	</outbound-rule>

</urlrewrite>
